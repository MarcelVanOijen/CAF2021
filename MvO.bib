@article{levyInferenceSpatialHeterogeneity2020,
  title = {Inference of Spatial Heterogeneity in Surface Fluxes from Eddy Covariance Data: {{A}} Case Study from a Subarctic Mire Ecosystem},
  shorttitle = {Inference of Spatial Heterogeneity in Surface Fluxes from Eddy Covariance Data},
