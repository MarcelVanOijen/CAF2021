@article{flechardCarbonNitrogenInteractions2020,
  title = {Carbon\textendash Nitrogen Interactions in {{European}} Forests and Semi-Natural Vegetation \textendash{} {{Part}} 2: {{Untangling}} Climatic, Edaphic, Management and Nitrogen Deposition Effects on Carbon Sequestration Potentials},
  shorttitle = {Carbon\textendash Nitrogen Interactions in {{European}} Forests and Semi-Natural Vegetation \textendash{} {{Part}} 2},
  author = {Flechard, Chris R. and van Oijen, Marcel and Cameron, David R. and de Vries, Wim and Ibrom, Andreas and Buchmann, Nina and Dise, Nancy B. and Janssens, Ivan A. and Neirynck, Johan and Montagnani, Leonardo and Varlagin, Andrej and Loustau, Denis and Legout, Arnaud and Ziembli{\'n}ska, Klaudia and Aubinet, Marc and Aurela, Mika and Chojnicki, Bogdan H. and Drewer, Julia and Eugster, Werner and Francez, Andr{\'e}-Jean and Juszczak, Rados{\l}aw and Kitzler, Barbara and Kutsch, Werner L. and Lohila, Annalea and Longdoz, Bernard and Matteucci, Giorgio and Moreaux, Virginie and Neftel, Albrecht and Olejnik, Janusz and Sanz, Maria J. and Siemens, Jan and Vesala, Timo and Vincke, Caroline and Nemitz, Eiko and {Zechmeister-Boltenstern}, Sophie and {Butterbach-Bahl}, Klaus and Skiba, Ute M. and Sutton, Mark A.},
  year = {2020},
  month = mar,
  volume = {17},
  pages = {1621--1654},
  publisher = {{Copernicus GmbH}},
  issn = {1726-4170},
  doi = {10.5194/bg-17-1621-2020},
  abstract = {{$<$}p{$><$}strong{$>$}Abstract.{$<$}/strong{$>$} The effects of atmospheric nitrogen deposition (\emph{N}\textsubscript{dep}) on carbon (C) sequestration in forests have often been assessed by relating differences in productivity to spatial variations of \emph{N}\textsubscript{dep} across a large geographic domain. These correlations generally suffer from covariation of other confounding variables related to climate and other growth-limiting factors, as well as large uncertainties in total (dry\&thinsp;+\&thinsp;wet) reactive nitrogen (\emph{N}\textsubscript{r}) deposition. We propose a methodology for untangling the effects of \emph{N}\textsubscript{dep} from those of meteorological variables, soil water retention capacity and stand age, using a mechanistic forest growth model in combination with eddy covariance CO\textsubscript{2} exchange fluxes from a Europe-wide network of 22 forest flux towers. Total \emph{N}\textsubscript{r} deposition rates were estimated from local measurements as far as possible. The forest data were compared with data from natural or semi-natural, non-woody vegetation sites.{$<$}/p{$>$} {$<$}p{$>$}The response of forest net ecosystem productivity to nitrogen deposition (dNEP / d\emph{N}\textsubscript{dep}) was estimated after accounting for the effects on gross primary productivity (GPP) of the co-correlates by means of a meta-modelling standardization procedure, which resulted in a reduction by a factor of about 2 of the uncorrected, apparent dGPP / d\emph{N}\textsubscript{dep} value. This model-enhanced analysis of the C and \emph{N}\textsubscript{dep} flux observations at the scale of the European network suggests a mean overall dNEP / d\emph{N}\textsubscript{dep} response of forest lifetime C sequestration to \emph{N}\textsubscript{dep} of the order of 40\textendash 50\&thinsp;g\&thinsp;C per g\&thinsp;N, which is slightly larger but not significantly different from the range of estimates published in the most recent reviews. Importantly, patterns of gross primary and net ecosystem productivity versus \emph{N}\textsubscript{dep} were non-linear, with no further growth responses at high \emph{N}\textsubscript{dep} levels (\emph{N}\textsubscript{dep}\&thinsp;\&gt;\&thinsp;2.5\textendash 3\&thinsp;g\&thinsp;N\&thinsp;m\textsuperscript{-2}\&thinsp;yr\textsuperscript{-1}) but accompanied by increasingly large ecosystem N losses by leaching and gaseous emissions. The reduced increase in productivity per unit N deposited at high \emph{N}\textsubscript{dep} levels implies that the forecast increased \emph{N}\textsubscript{r} emissions and increased \emph{N}\textsubscript{dep} levels in large areas of Asia may not positively impact the continent's forest CO\textsubscript{2} sink. The large level of unexplained variability in observed carbon sequestration efficiency (CSE) across sites further adds to the uncertainty in the dC/dN response.{$<$}/p{$>$}},
  file = {/Users/marcelvanoijen/OneDrive/Documents/Zotero_attachments/Flechard et al_2020_Carbon–nitrogen interactions in European forests and semi-natural vegetation –2.pdf},
  journal = {Biogeosciences},
  language = {English},
  number = {6}
}

