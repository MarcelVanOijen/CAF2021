@article{Thorp_1993_Architectural,
  title = {Architectural Analysis of Tree Form in a Range of Avocado Cultivars},
  author = {Thorp, G.T. and Sedgley, M.},
  year = {1993},
  month = jan,
  volume = {53},
  pages = {85--98},
