@article{Salazar-Garcia_2011_Iinfluence,
  title = {Iinfluence of Climate, Soil Moisture, and Flowering Phenology on Biomass and Nutrient Composition of `{{Hass}}' Avocado Fruit in {{Michoac\'an}}, {{Mexico}}},
  author = {{Salazar-Garc{\'i}a}, Samuel and {Gonz{\'a}lez-Dur{\'a}n}, Isidro Jos{\'e} Luis and {Tapia-Vargas}, Luis Mario},
  year = {2011},
  month = aug,
  volume = {17},
  pages = {183--194},
  issn = {1027152X, 20074034},
  doi = {10.5154/r.rchsh.2011.17.020},
  abstract = {The extended harvest season of `Hass' avocado is due to the diversity of cultivation conditions and the occurrence of multiple vegetative flushes resulting in several bloom and harvest periods. The objective of this study was to evaluate the influence of three climatic conditions (subhumid semiwarm, humid semiwarm, and subhumid temperate), soil moisture regimes (with and without irrigation), and flowering period (``crazy'': August-September; ``normal'': December-February; ``marce\~na'': February-March), on biomass of different fruit parts and their nutrient composition. Six trees were selected from each of the six commercial orchards, choosing ten fruit of each tree per flowering season ({$\geq$}21.5 \% pulp dry matter). Fruits were separated in epidermis, pulp, testa, and embryo. Fresh weight (FW), dry weight (DW), dry matter (DM), and nutrient composition were determined for each part. Different to climate and soil moisture regimes, flowering period affected FW, DW, and DM of the fruit parts, as well as their nutrient composition, especially, K, Ca, Mg, S, Fe, Cu, Mn, Zn, and B. Under subhumid climate, the highest contents of N and Mn were obtained in epidermis; of N, P, K, Ca, and Mg in pulp; of Mg, S, Cu, and Mn in testa; and B in testa and embryo. Non-irrigated orchards had higher concentrations of Ca, S, and Cu in epidermis; P, K, Mn, Ca, and Cu in pulp; K, Mn, Cu in testa; and P, K, Mn in embryo.},
  file = {/Users/marcelvanoijen/OneDrive/Documents/Zotero_attachments/Salazar-García et al_2011_Iinfluence of climate, soil moisture, and flowering phenology on biomass and.pdf},
  journal = {Revista Chapingo Serie Horticultura},
